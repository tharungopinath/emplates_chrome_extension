{
  "manifest_version": 3,
  "name": "NetworkFlow",
  "version": "1.0",
  "description": "Email templates for networking.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://outlook.live.com/*",
        "https://outlook.office.com/*",
        "https://outlook.office365.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}